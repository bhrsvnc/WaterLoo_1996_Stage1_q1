def classify_number(n):
    sum_of_divisors = 0
    for i in range(1, n):
        if n % i == 0:
            sum_of_divisors += i

    if sum_of_divisors < n:
        print(n, "is a deficient number.")
    elif sum_of_divisors > n:
        print(n, "is an abundant number.")
    else:
        print(n, "is a perfect number.")

turn = int(input())
for i in range(turn):
    n = int(input()) 
    classify_number(n)